{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "CommonJS",
    "target": "ES2017",
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "baseUrl": "./",
    "paths": {
      "src/*": ["../src/*"],
      ".data/*": ["../.data/*"],
      ".mocks/*": ["../.mocks/*"],
      ".shims/*": ["../.shims/*"],
    },
    // "outDir": "./out",
  },
  "exclude": ["node_modules"]
}